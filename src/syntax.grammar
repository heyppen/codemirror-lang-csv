@top Program { expression* }

@skip { newline | Comment }

expression {
  String |
  Separator |
  Value
}

@tokens {
  Separator { $[,] }

  Comment { "//" ![\n]* }

  Value { ![,\n]+ }
  
  String { '"' (!["\\] | "\\" _)* '"' }

  newline { $[\n]}

  @precedence { String, Separator, Value }
}

@detectDelim
